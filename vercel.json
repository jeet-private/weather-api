{
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            },
            {
                "key": "Cache-Control",
                "value": "no-cache, no-store, must-revalidate"
            },
            {
                "key": "Open-Source",
                "value": "https://github.com/cachecleanerjeet/weather-api"
            }
        ]
    }]
}